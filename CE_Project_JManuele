        "df_nhl_set4.drop(df_nhl_set4[df_nhl_set4[\"Pos\"]!=\" D \"].index, inplace=True)\n",
        "df_nhl_set4[\"HIT/GP\"]= df_nhl_set4[\"HIT\"]/df_nhl_set4[\"GP\"]\n",
        "df_nhl_set4[\"BLK/GP\"]= df_nhl_set4[\"BLK\"]/df_nhl_set2[\"GP\"]"
